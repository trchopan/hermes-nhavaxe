{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "hermes": {
      "root": "projects/hermes",
      "sourceRoot": "projects/hermes/src",
      "projectType": "application",
      "prefix": "hm",
      "schematics": {
        "@schematics/angular:component": {
          "styleext": "scss"
        }
      },
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/public/hermes",
            "index": "projects/hermes/src/index.html",
            "main": "projects/hermes/src/main.ts",
            "polyfills": "projects/hermes/src/polyfills.ts",
            "tsConfig": "projects/hermes/tsconfig.app.json",
            "assets": [
              "projects/hermes/src/favicon.ico",
              "projects/hermes/src/assets",
              "projects/hermes/src/manifest.json"
            ],
            "styles": [
              {
                "input": "node_modules/@angular/material/prebuilt-themes/indigo-pink.css"
              },
              "projects/hermes/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "projects/hermes/src/environments/environment.ts",
                  "with": "projects/hermes/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "hermes:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "hermes:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "hermes:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/hermes/src/test.ts",
            "polyfills": "projects/hermes/src/polyfills.ts",
            "tsConfig": "projects/hermes/tsconfig.spec.json",
            "karmaConfig": "projects/hermes/karma.conf.js",
            "styles": [
              {
                "input": "node_modules/@angular/material/prebuilt-themes/indigo-pink.css"
              },
              "projects/hermes/src/styles.scss"
            ],
            "scripts": [],
            "assets": [
              "projects/hermes/src/favicon.ico",
              "projects/hermes/src/assets",
              "projects/hermes/src/manifest.json"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/hermes/tsconfig.app.json",
              "projects/hermes/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "hermes-e2e": {
      "root": "projects/hermes-e2e/",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "projects/hermes-e2e/protractor.conf.js",
            "devServerTarget": "hermes:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "hermes:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": "projects/hermes-e2e/tsconfig.e2e.json",
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "hermes-admin": {
      "root": "projects/hermes-admin/",
      "sourceRoot": "projects/hermes-admin/src",
      "projectType": "application",
      "prefix": "hm-admin",
      "schematics": {
        "@schematics/angular:component": {
          "styleext": "scss"
        }
      },
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/hermes-admin",
            "index": "projects/hermes-admin/src/index.html",
            "main": "projects/hermes-admin/src/main.ts",
            "polyfills": "projects/hermes-admin/src/polyfills.ts",
            "tsConfig": "projects/hermes-admin/tsconfig.app.json",
            "assets": [
              "projects/hermes-admin/src/favicon.ico",
              "projects/hermes-admin/src/assets"
            ],
            "styles": [
              {
                "input": "node_modules/@angular/material/prebuilt-themes/indigo-pink.css"
              },
              "projects/hermes-admin/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "projects/hermes-admin/src/environments/environment.ts",
                  "with": "projects/hermes-admin/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "hermes-admin:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "hermes-admin:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "hermes-admin:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/hermes-admin/src/test.ts",
            "polyfills": "projects/hermes-admin/src/polyfills.ts",
            "tsConfig": "projects/hermes-admin/tsconfig.spec.json",
            "karmaConfig": "projects/hermes-admin/karma.conf.js",
            "styles": [
              {
                "input": "node_modules/@angular/material/prebuilt-themes/indigo-pink.css"
              },
              "projects/hermes-admin/src/styles.scss"
            ],
            "scripts": [],
            "assets": [
              "projects/hermes-admin/src/favicon.ico",
              "projects/hermes-admin/src/assets"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/hermes-admin/tsconfig.app.json",
              "projects/hermes-admin/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "hermes-admin-e2e": {
      "root": "projects/hermes-admin-e2e/",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "projects/hermes-admin-e2e/protractor.conf.js",
            "devServerTarget": "hermes-admin:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "hermes-admin:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": "projects/hermes-admin-e2e/tsconfig.e2e.json",
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }
  },
  "defaultProject": "hermes"
}