{
  "name": "hermes",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --live-reload=false -o --host=0.0.0.0",
    "serve:editor": "ng serve hermes-editor --live-reload=false -o --host=0.0.0.0",
    "build": "ng build --prod",
    "build:editor": "ng build hermes-editor --prod",
    "build:nhavaxe-publish": "cp projects/hermes-editor/src/environments/firebase.nhavaxe.ts projects/hermes-editor/src/environments/firebase.conf.ts && ng build hermes --prod",
    "build:nhavaxe-editor": "cp projects/hermes-editor/src/environments/firebase.nhavaxe.ts projects/hermes-editor/src/environments/firebase.conf.ts && ng build hermes-editor --prod",
    "deploy:hermes": "cp firebase.hermes.json firebase.json && firebase deploy -P default",
    "deploy:editor": "cp firebase.editor.json firebase.json && firebase deploy -P editor",
    "deploy:nhavaxe-publish": "cp firebase.hermes.json firebase.json && firebase deploy -P nhavaxe-publish",
    "deploy:nhavaxe-editor": "cp firebase.editor.json firebase.json && firebase deploy -P nhavaxe-editor",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^6.0.4",
    "@angular/core": "^6.0.4",
    "@angular/compiler": "^6.0.4",
    "@angular/animations": "^6.0.4",
    "@angular/cdk": "^6.2.1",
    "@angular/elements": "^6.0.4",
    "@angular/forms": "^6.0.4",
    "@angular/http": "^6.0.4",
    "@angular/material": "^6.2.1",
    "@angular/platform-browser": "^6.0.4",
    "@angular/platform-browser-dynamic": "^6.0.4",
    "@angular/pwa": "^0.6.8",
    "@angular/router": "^6.0.4",
    "angularfire2": "^5.0.0-rc.10",
    "core-js": "^2.5.7",
    "firebase": "^5.0.4",
    "ngx-quill": "^3.2.0",
    "quill": "^1.3.6",
    "rxjs": "^6.2.0",
    "rxjs-compat": "^6.2.0",
    "screenfull": "^3.3.2",
    "zone.js": "^0.8.26",
    "@angular/service-worker": "^6.0.4"
  },
  "devDependencies": {
    "@angular/compiler-cli": "^6.0.4",
    "@angular-devkit/build-angular": "^0.6.8",
    "typescript": "~2.7.2",
    "@angular-devkit/build-ng-packagr": "~0.6.8",
    "ng-packagr": "^3.0.0-rc.2",
    "tsickle": ">=0.25.5",
    "tslib": "^1.7.1",
    "@angular/cli": "^6.0.8",
    "@angular/language-service": "^6.0.4",
    "@types/jasmine": "^2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.2.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^2.0.2",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^1.4.3",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.3.0",
    "ts-node": "~5.0.1",
    "tslint": "~5.9.1"
  }
}